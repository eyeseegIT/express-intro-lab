<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Lab App</title>
</head>
<body>
  <h1>Federal Holidays 2021</h1>
  <% if(error) { %>
    <h2><%= error %></h2>
  <% } %>
  <ul>
    <% fedHolidays.forEach(function(holiday) { %>
      <li>
        <%= holiday.day %>
          - 
        <%= holiday.observed ? 'observed' : 'not observed' %>
      </li>
    <% }) %>
  </ul>
</body>
</html>